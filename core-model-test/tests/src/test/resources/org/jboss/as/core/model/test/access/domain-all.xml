<?xml version='1.0' encoding='UTF-8'?>
<domain xmlns="urn:jboss:domain:2.0">
    <management>
        <access-control>
            <server-group-scoped-roles>
                <role name="a" base-role="Deployer">
                    <server-group name="main-server-group"/>
                    <server-group name="other-server-group"/>
                </role>
                <role name="b" base-role="Administrator">
                    <server-group name="other-server-group"/>
                </role>
            </server-group-scoped-roles>
            <host-scoped-roles>
                <role name="c" base-role="Deployer">
                    <host name="master"/>
                    <host name="slave"/>
                </role>
                <role name="d" base-role="Administrator"/>
            </host-scoped-roles>
            <constraints>
                <vault-expression-sensitivity requires-access="false" requires-read="false" requires-write="false"/>
                <sensitive-classifications>
                    <sensitive-classification name="security-realm">
                        <type name="core" requires-access="false" requires-read="false" requires-write="false"/>
                    </sensitive-classification>
                </sensitive-classifications>
                <application-types>
                    <application-type name="deployment">
                        <type name="core" application="false"/>
                    </application-type>
                </application-types>
            </constraints>
        </access-control>
    </management>
</domain>
