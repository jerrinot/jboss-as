<?xml version='1.0' encoding='UTF-8'?>
<server xmlns="urn:jboss:domain:2.0">
   <management>
      <access-control>
         <role-mapping use-realm-roles="true">
            <role name="Monitor">
               <include>
                  <user realm="ManagementRealm" name="UserOne" />
                  <group realm="ApplicationRealm" name="GroupOne" />
               </include>
               <exclude>
                  <user realm="ManagementRealm" name="UserTwo" />
                  <group realm="ApplicationRealm" name="GroupTwo" />
               </exclude>
            </role>
            <role name="Operator">
               <include>
                  <user name="UserThree" />
                  <group name="GroupThree" />
               </include>
            </role>
            <role name="Maintainer">
               <exclude>
                  <user name="UserFour" />
                  <group name="GroupFour" />
               </exclude>
            </role>
         </role-mapping>

         <constraints>
            <vault-expression-sensitivity
               requires-access="false" requires-read="false"
               requires-write="false" />
            <sensitive-classifications>
               <sensitive-classification name="security-realm">
                  <type name="core" requires-access="false"
                     requires-read="false" requires-write="false" />
               </sensitive-classification>
            </sensitive-classifications>
            <application-types>
               <application-type name="deployment">
                  <type name="core" application="false" />
               </application-type>
            </application-types>
         </constraints>
      </access-control>
   </management>
</server>
